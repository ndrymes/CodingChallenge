{
    "name": "softotech",
    "version": "1.0.0",
    "description": "handler",
    "main": "index.js",
    "scripts": {
        "start": "node ./dist/src/server.js",
        "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node'  src/server.ts",
        "lint": "eslint --ignore-path .eslintignore --ext .ts,.js .",
        "lint:fix": "eslint --ignore-path .eslintignore --ext .ts,.js --fix .",
        "test:all": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests",
        "test": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests/unit",
        "test-integration": "cross-env NODE_ENV=test jest --verbose --runInBand --collectCoverage=true ./tests/integration",
        "test-integration:dockest": "cross-env NODE_ENV=test ts-node ./dockest.ts",
        "build": "rm -rf dist && tsc",
        "format": "prettier --config .prettierrc.js --write './**/*.{js,ts,json,yml,yaml}'",
        "format:check": "prettier --config .prettierrc.js --check './**/*.{js,ts,json,yml,yaml}'",
        "generate-error-map": "ts-node src/cmd/generate-error-map generate",
        "generate-client:node-docker": "docker run --rm -v ${PWD}:/local openapitools/openapi-generator-cli generate -i /local/docs/openapi.yaml -g typescript-axios --additional-properties=modelPropertyNaming=original,npmName=@boxbag/business-banking-ekyb -o /local/clients/node",
        "generate-client:node": "npm run generate-client:node-docker && sed -i '' 's+./dist+dist+g' clients/node/package.json"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/jest": "^29.2.2",
        "ajv": "^8.12.0",
        "ajv-errors": "^3.0.0",
        "bcrypt": "^5.1.0",
        "compression": "^1.7.4",
        "connect-redis": "^7.0.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "dotenv": "^10.0.0",
        "express": "^4.18.2",
        "express-http-context": "^1.2.4",
        "express-session": "^1.17.3",
        "flatted": "^3.2.7",
        "helmet": "^4.6.0",
        "http-graceful-shutdown": "^3.1.3",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mongoose": "^7.0.3",
        "pg": "^8.7.1",
        "redis": "^4.6.5",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.19",
        "supertest": "^6.3.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.11",
        "@types/express-session": "^1.17.7",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/module-alias": "^2.0.1",
        "@types/pg": "^8.6.5",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.27.0",
        "eslint-plugin-chai-expect": "^3.0.0",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-mocha": "^10.1.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-varspacing": "^1.2.2",
        "jest": "^26.6.3",
        "jest-extended": "^0.11.5",
        "nodemon": "^2.0.20",
        "ts-jest": "^26.5.6",
        "ts-node": "^10.8.1",
        "ts-node-dev": "^1.1.8",
        "tsc-watch": "^4.5.0",
        "typescript": "4.5.2",
        "winston": "^3.8.2"
    }
}
